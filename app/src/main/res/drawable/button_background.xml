<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="0dip" />
            <stroke android:width="0.5dip" android:color="#535353" />
            <gradient android:angle="0" android:startColor="#d2d2d2" android:endColor="#d2d2d2"  />
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="0dip" />
            <stroke android:width="0.5dip" android:color="#535353" />
            <gradient android:angle="00" android:startColor="#ffffff" android:endColor="#ffffff" />
        </shape>
    </item>
</selector>